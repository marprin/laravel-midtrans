{
    "name": "marprinhm/midtrans",
    "description": "A Midtrans Payment Wrapper for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel", "payment", "midtrans", "veritrans"
    ],
    "authors": [
        {
            "name": "Marprin Hennes Muchri",
            "email": "marprin93@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x|5.8.x",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Marprinhm\\Midtrans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.4-dev"
        },
        "laravel": {
            "providers": [
                "Marprinhm\\Midtrans\\MidtransServiceProvider"
            ],
            "aliases": {
                "Midtrans": "Marprinhm\\Midtrans\\Facades\\Midtrans",
                "Veritrans": "Marprinhm\\Midtrans\\Facades\\Veritrans"
            }
        }
    },
    "config":{
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
