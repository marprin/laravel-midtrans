{
    "name": "marprinhm/midtrans",
    "description": "A Midtrans Payment Wrapper for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "payment",
        "midtrans",
        "veritrans"
    ],
    "authors": [
        {
            "name": "Marprin Hennes Muchri",
            "email": "marprin93@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "6.x",
        "guzzlehttp/guzzle": "^6.4"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~8.0",
        "vlucas/phpdotenv": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Marprinhm\\Midtrans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "laravel": {
            "providers": [
                "Marprinhm\\Midtrans\\MidtransServiceProvider"
            ],
            "aliases": {
                "Midtrans": "Marprinhm\\Midtrans\\Facades\\Midtrans",
                "Veritrans": "Marprinhm\\Midtrans\\Facades\\Veritrans"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}